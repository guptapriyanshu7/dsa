{
  "C_Cpp.updateChannel": "Insiders",
  "code-runner.runInTerminal": true,
  "git.ignoreMissingGitWarning": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "files.autoSaveDelay": 1,
  "code-runner.executorMap": {
    "javascript": "node",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "php": "php",
    "python": "python -u",
    "perl": "perl",
    "perl6": "perl6",
    "ruby": "ruby",
    "go": "go run",
    "lua": "lua",
    "groovy": "groovy",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "shellscript": "bash",
    "fsharp": "fsi",
    "csharp": "scriptcs",
    "vbscript": "cscript //Nologo",
    "typescript": "ts-node",
    "coffeescript": "coffee",
    "scala": "scala",
    "swift": "swift",
    "julia": "julia",
    "crystal": "crystal",
    "ocaml": "ocaml",
    "r": "Rscript",
    "applescript": "osascript",
    "clojure": "lein exec",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "racket": "racket",
    "scheme": "csi -script",
    "ahk": "autohotkey",
    "autoit": "autoit3",
    "dart": "dart",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "haskell": "runhaskell",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "lisp": "sbcl --script",
    "kit": "kitc --run",
    "v": "v run",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css"
  },
  "editor.formatOnSave": true,
  "python.defaultInterpreterPath": "C:\\Python39\\python.exe",
  "kite.showWelcomeNotificationOnStartup": false,
  "settingsSync.ignoredSettings": [
    "-python.defaultInterpreterPath",
    "-C_Cpp.clang_format_fallbackStyle",
    "-C_Cpp.clang_format_path",
    "-C_Cpp.clang_format_style",
    "-php.validate.executablePath"
  ],
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.languageServer": "Microsoft",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "workbench.iconTheme": "material-icon-theme",
  "dart.debugExternalLibraries": false,
  "dart.debugSdkLibraries": false,
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "dart.warnWhenEditingFilesOutsideWorkspace": false,
  "java.configuration.checkProjectSettingsExclusions": false,
  "editor.tabSize": 2,
  "explorer.confirmDragAndDrop": false,
  "jupyter.alwaysTrustNotebooks": true,
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "python.formatting.provider": "none",
  "dart.previewLsp": true,
  "php.validate.executablePath": "C:/xampp/php/php.exe",
  "editor.fontFamily": "Hasklig",
  "editor.fontLigatures": true,
  "editor.tokenColorCustomizations": {
    "[Sublime Material Theme - Dark]": {
      "comments": "#00ccff"
    }
    // "textMateRules": [
    //   {
    //     "scope": [
    //       //following will be in italic (=FlottFlott)
    //       "comment",
    //       // "entity.name.type.class", //class names
    //       "keyword", //import, export, return…
    //       "constant", //String, Number, Boolean…, this, super
    //       "storage.modifier", //static keyword
    //       "storage.type.class.js" //class keyword
    //     ],
    //     "settings": {
    //       "fontStyle": "italic"
    //     }
    //   },
    //   {
    //     "scope": [
    //       //following will be excluded from italics (VSCode has some defaults for italics)
    //       "invalid",
    //       "keyword.operator",
    //       "constant.numeric.css",
    //       "keyword.other.unit.px.css",
    //       "constant.numeric.decimal.js",
    //       "constant.numeric.json"
    //     ],
    //     "settings": {
    //       "fontStyle": ""
    //     }
    //   }
    // ]
  },
  "workbench.colorCustomizations": {
    //   "editor.selectionBackground": "#00fff225",
    //   "editor.selectionHighlightBackground": "#00fff220",
    "gitDecoration.untrackedResourceForeground": "#D4B1F3"
  },
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.previewFlutterUiGuides": true,
  "workbench.startupEditor": "none",
  "C_Cpp.formatting": "vcFormat",
  "C_Cpp.vcFormat.newLine.beforeCatch": false,
  "C_Cpp.vcFormat.newLine.beforeElse": false,
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "sameLine",
  "workbench.editor.enablePreview": false,
  "workbench.editor.untitled.hint": "hidden",
  "explorer.confirmDelete": false,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "terminal.integrated.fontFamily": "Cascadia Mono",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.lnk": "default"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "explorer.compactFolders": false,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "Mayukai Mono",
  "redhat.telemetry.enabled": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "git.alwaysSignOff": true,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "gopls": {
    "experimentalWorkspaceModule": true
  },
  "go.toolsManagement.autoUpdate": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.panel.defaultLocation": "right",
  "C_Cpp.vcFormat.indent.caseLabels": true,
  "git.enableSmartCommit": true,
  "files.defaultLanguage": "${activeEditorLanguage},",
  "git.confirmSync": false,
  "leetcode.workspaceFolder": "c:\\Files\\Programs\\leetcode",
  "leetcode.defaultLanguage": "cpp",
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.hint.commentDescription": false,
  "editor.bracketPairColorization.enabled": true,
  // Cppcheck
  "cpp-check-lint.cppcheck.--enable=": "warning,style,performance,portability,information,missingInclude",
  "c-cpp-flylint.cppcheck.suppressions": [
    "misc-implicitlyVirtual",
    "unmatchedSuppression",
    "missingIncludeSystem",
    "constParameter"
  ],
  // Cpplint
  // "cpp-check-lint.cpplint.--filter=": "-whitespace/comma, -whitespace/ending_newline, -legal, -whitespace/braces, -whitespace/indent, -whitespace/newline,-build/namespace, -build/c++11, -build/include_subdir, -build/include_order, -build/header_guard, -readability/alt_tokens, -readability/casting, -runtime/references",
  "c-cpp-flylint.flexelint.enable": false,
  "c-cpp-flylint.lizard.enable": false,
  "c-cpp-flylint.flawfinder.enable": false,
  "c-cpp-flylint.clang.enable": false,
  "c-cpp-flylint.cppcheck.standard": [
    "c11",
    "c++17"
  ],
  "c-cpp-flylint.cppcheck.inconclusive": true,
  "c-cpp-flylint.cppcheck.addons": [
    "cert",
    "misc"
  ],
  "code-runner.languageIdToFileExtensionMap": {
    "bat": ".bat",
    "powershell": ".ps1",
    "typescript": ".ts"
  },
  "c-cpp-flylint.cppcheck.enable": true,
  "[cpp]": {
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd",
  },
  "files.autoSave": "afterDelay",
  "c-cpp-flylint.cppcheck.severityLevels": {
    "error": "Error",
    "warning": "Warning",
    "style": "Information",
    "performance": "Information",
    "portability": "Information",
    "information": "Information",
  },
  "cmake.configureOnOpen": true,
  "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
  "terminal.integrated.profiles.windows": {
    // "C:\\Program Files\\PowerShell\\7\\pwsh.exe (migrated)": {
    //   "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    //   "args": []
    // }
  },
}